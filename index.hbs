    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta
      property="og:description"
      content="North by Northwestern's Class of {{year}} seniors reflect on their time at Northwestern and what comes next."
    />
    <meta property="og:title" content="Commencement {{year}} | North by Northwestern" />
    <meta property="og:url" content="{{articleUrl}}" />
    <link rel="shortcut icon" href="https://media.northbynorthwestern.com/media/img/favicon-hi.ico" />

    <title>Commencement {{year}} | North by Northwestern</title>
    <script>
      function equalheight(container) {
        var currentTallest = 0,
          currentRowStart = 0,
          rowDivs = [],
          elem = null,
          topPosition = 0;

        var elemArr = Array(...document.getElementsByClassName("grid-entry"));
        elemArr.forEach((c) => {
          elem = c;
          elem.style.height = "auto";
          topPostion = elem.style.topPosition;

          if (currentRowStart != topPostion) {
            for (currentDiv = 0; currentDiv < rowDivs.length; currentDiv++) {
              rowDivs[currentDiv].height(currentTallest);
            }
            rowDivs.length = 0; // empty the array
            currentRowStart = topPostion;
            currentTallest = elem.style.height;
            rowDivs.push(elem);
          } else {
            rowDivs.push(elem);
            currentTallest = currentTallest < elem.style.height ? elem.style.height : currentTallest;
          }
          for (currentDiv = 0; currentDiv < rowDivs.length; currentDiv++) {
            rowDivs[currentDiv].style.height = currentTallest;
          }
        });
      }

      window.addEventListener("load", (e) => {
        equalheight(".grid-entry");
      });

      window.addEventListener("resize", (e) => {
        equalheight(".grid-entry");
      });
    </script>
  </head>
